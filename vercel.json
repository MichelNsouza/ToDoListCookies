{
  "version": 2,
  "builds": [
      { "src": "/api/index.php", "use": "vercel-php@0.6.0" },
      { "src": "/**", "use": "@vercel/static" }
  ],
  "routes": [
      {
          "src": "/(css|js|images)/(.*)",
          "dest": "assets/$1/$2"
      },
      {
          "src": "/(.*)",
          "dest": "/api/index.php"
      }
  ]
}
